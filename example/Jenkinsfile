pipeline {
  agent any
  stages {
    stage('Init') {
      parallel {
        stage('Init') {
          steps {
            echo 'Hello World'
            lock(resource: 'a') {
              echo 'Lock'
            }

          }
        }

        stage('What') {
          environment {
            remote = '[:}'
          }
          steps {
            echo 'Do something'
            bat(returnStatus: true, script: 'echo 1', returnStdout: true)
          }
        }

      }
    }

  }
  environment {
    var1 = 'a1'
  }
}